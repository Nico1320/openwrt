#include "qcom-ipq4019.dtsi"


/ {
	model = "Qualcomm Technologies, Inc. IPQ4019/AP-DK01.1";
	compatible = "qcom,ipq4019";

	clocks {
                xo: xo {
                        compatible = "fixed-clock";
                        clock-frequency = <48000000>;
                        #clock-cells = <0>;
                };
	};

	soc {


		timer {
			compatible = "arm,armv7-timer";
			interrupts = <1 2 0xf08>,
				     <1 3 0xf08>,
				     <1 4 0xf08>,
				     <1 1 0xf08>;
			clock-frequency = <48000000>;
		};

		pinctrl@0x01000000 {
			mdio_pins: mdio_pinmux {
				pinmux_1 {
					pins = "gpio53";
					function = "mdio1";
				};
				pinmux_2 {
					pins = "gpio52";
					function = "mdc";
				};
				pinconf {
					pins = "gpio52", "gpio53";
					bias-pull-up;
				};
			};

			serial_pins: serial_pinmux {
				mux {
					pins = "gpio60", "gpio61";
					function = "blsp_uart0";
					bias-disable;
				};
			};

			spi_0_pins: spi_0_pinmux {
				pinmux {
					function = "blsp_spi0";
					pins = "gpio55", "gpio56", "gpio57";
				};
				pinmux_cs {
					function = "gpio";
					pins = "gpio54", "gpio59";
				};
				pinconf {
					pins =   "gpio55", "gpio56", "gpio57";
					/*drive-strength = <12>;*/
					bias-disable;
				};
				pinconf_cs {
					pins = "gpio54", "gpio59";
					drive-strength = <2>;
					bias-disable;
					output-high;
					/*bias-pull-up;*/
				};
			};
		};

		blsp_dma: dma@7884000 {
			status = "ok";
		};

		spi_0: spi@78b5000 {
			pinctrl-0 = <&spi_0_pins>;
			pinctrl-names = "default";
			status = "ok";

			cs-gpios = <&tlmm 54 0>,
					<&tlmm 59 0>;

			mx25l25635e@0 {
				status = "disabled";
			};

			m25p80@0 {
				#address-cells = <1>;
				#size-cells = <0>;
				reg = <0>;
				compatible = "mx25l1606e", "n25q128a11";
				linux,modalias = "m25p80", "mx25l1606e", "n25q128a11";
				spi-max-frequency = <24000000>;
			};

			mt29f@1 {
				status = "ok";
				#address-cells = <1>;
				#size-cells = <0>;
				compatible = "spinand,mt29f", "w25n01gv";
				reg = <1>;
				spi-max-frequency = <24000000>;
			};
		};
		
		counter@4a1000 {
			compatible = "qcom,qca-gcnt";
			reg = <0x4a1000 0x4>;
		};
		
		mdio@90000 {
			status = "okay";
		};
		
		ess-switch@c000000 {
			status = "ok";
		};

		serial@78af000 {
			pinctrl-0 = <&serial_pins>;
			pinctrl-names = "default";
			status = "ok";
		};

		cryptobam: dma@8e04000 {
			status = "ok";
		};

		crypto@8e3a000 {
			status = "ok";
		};

		watchdog@b017000 {
			status = "ok";
		};

		usb3_ss_phy: ssphy@9a000 {
			status = "ok";
		};

		usb3_hs_phy: hsphy@a6000 {
			status = "ok";
		};

		usb3: usb3@8af8800 {
			status = "ok";
		};

		usb2_hs_phy: hsphy@a8000 {
			status = "ok";
		};

		usb2: usb2@60f8800 {
			status = "ok";
		};

		usb3_ss_phy: ssphy@9a000 {
			status = "ok";
		};

		usb3_hs_phy: hsphy@a6000 {
			status = "ok";
		};

		usb3: usb3@8af8800 {
			status = "ok";
		};

		usb2_hs_phy: hsphy@a8000 {
			status = "ok";
		};

		usb2: usb2@60f8800 {
			status = "ok";
		};
		
		wifi@a000000 {
			status = "okay";
		};

		wifi@a800000 {
			status = "okay";
		};

		usb3_ss_phy: ssphy@9a000 {
			status = "ok";
		};

		usb3_hs_phy: hsphy@a6000 {
			status = "ok";
		};

		usb3: usb3@8af8800 {
			status = "ok";
		};

		usb2_hs_phy: hsphy@a8000 {
			status = "ok";
		};

		usb2: usb2@60f8800 {
			status = "ok";
		};

		usb3_ss_phy: ssphy@9a000 {
			status = "ok";
		};

		usb3_hs_phy: hsphy@a6000 {
			status = "ok";
		};

		usb3: usb3@8af8800 {
			status = "ok";
		};

		usb2_hs_phy: hsphy@a8000 {
			status = "ok";
		};

		usb2: usb2@60f8800 {
			status = "ok";
		};
	};
};
